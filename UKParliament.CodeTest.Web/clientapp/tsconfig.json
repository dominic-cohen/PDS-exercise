{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [ "dom", "dom.iterable", "esnext" ],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": [ "src" ],
  "exclude": [ "node_modules" ]
}
